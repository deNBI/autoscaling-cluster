{
  "extends": [
    "config:base",
    ":automergePatch"
  ],
 "packageRules": [
    {
      "updateTypes": ["major"],
      "addLabels": ["major"],
       "pythonVersion": "3.8"
    },
    {
      "updateTypes": ["minor"],
      "addLabels": ["minor"],
      "pythonVersion": "3.8"

    },
     {
      "updateTypes": ["patch"],
      "addLabels": ["patch"],
      "pythonVersion": "3.8"

    },
    {
      "updateTypes": ["pin"],
      "addLabels": ["pin"],
      "pythonVersion": "3.8"

    }
   ],
  "vulnerabilityAlerts": {
    "labels": ["security"]
  },
  "baseBranches": ["dev"],
  "reviewers": ["team:portal-dev"],
  "labels": ["dependencies"],
  "assignees": ["team:portal-dev"],
  "branchPrefix": "deps/",
  "bumpVersion": "minor",
  "rebaseWhen": "behind-base-branch",
  "commitBodyTable": "true",
  "commitMessagePrefix": "feat(Dependencies):"
}
